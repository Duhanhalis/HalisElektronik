@model IEnumerable<SocialMedia>?
@if (Model.Count() > 0)
{

    <ul class="socialMedia">
        @foreach (var item in Model)
        {
            <li>
                <a href="@item.Link">
                    <img src="https://localhost:7273/api/Image/GetImageByName/@(item.Images.ImageUrl)" alt="Alternate Text" />
                    <p>@item.Title</p>
                </a>
            </li>
        }
    </ul>
}
else
{
    <ul class="socialMedia">
        <li>
            <a href="">
                <img src="https://localhost:7273/api/Image/GetImageByName/No-Image.jpg" alt="Alternate Text" />
                <p></p>
            </a>
        </li>
    </ul>
}